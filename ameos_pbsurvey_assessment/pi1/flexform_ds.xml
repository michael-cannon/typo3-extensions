<T3DataStructure>
	<meta>
		<langChildren>1</langChildren>
	    <langDisable>0</langDisable>
	</meta>
		<sheets>
			<sheetStd>
				<ROOT>
					<TCEforms>
						<sheetTitle>LLL:EXT:ameos_pbsurvey_assessment/locallang_db.xml:flexform.general</sheetTitle>
					</TCEforms>
					<type>array</type>
					<el>
						<score>
							<TCEforms>
								<label>LLL:EXT:ameos_pbsurvey_assessment/locallang_db.xml:flexform.displayscore</label>
								<config>
									<type>check</type>
								</config>
							</TCEforms>
						</score>
						<pourcent>
							<TCEforms>
								<label>LLL:EXT:ameos_pbsurvey_assessment/locallang_db.xml:flexform.displaypercent</label>
								<config>
									<type>check</type>
								</config>
							</TCEforms>
						</pourcent>
						<pageretour>
							<TCEforms>
								<label>LLL:EXT:ameos_pbsurvey_assessment/locallang_db.xml:flexform.explanation</label>
								<config>
									<type>group</type>
									<internal_type>db</internal_type>
									<allowed>pages</allowed>
									<size>1</size>
									<maxitems>1</maxitems>
									<minitems>0</minitems>
									<show_thumbs>1</show_thumbs>
									<!--eval>required,titre</eval-->
								</config>
							</TCEforms>
						</pageretour>
						</el>
					</ROOT>
				</sheetStd>
				<sheet>
					<ROOT>
						<TCEforms>
							<sheetTitle>LLL:EXT:ameos_pbsurvey_assessment/locallang_db.xml:flexform.stage</sheetTitle>
						</TCEforms>
					<type>array</type>
					<el>
						<palliers>
							<section>1</section>
							<type>array</type>
							<tx_templavoila>
								<title></title>
							</tx_templavoila>
							<el>
								<pallier>
									<type>array</type>
									<tx_templavoila>
										<title>LLL:EXT:ameos_pbsurvey_assessment/locallang_db.xml:flexform.stage</title>
									</tx_templavoila>
									<el>
										<niveau>
											<TCEforms>
												<label>LLL:EXT:ameos_pbsurvey_assessment/locallang_db.xml:flexform.level</label>
												<config>
													<type>input</type>
													<size>20</size>
													<eval>required,int</eval>
												</config>
											</TCEforms>
										</niveau>
										<titre>
											<TCEforms>
												<label>LLL:EXT:ameos_pbsurvey_assessment/locallang_db.xml:flexform.title</label>
												<config>
													<type>input</type>
													<size>24</size>
													<eval>required</eval>
												</config>
											</TCEforms>
										</titre>
										<contenu>
											<TCEforms>
												<label>LLL:EXT:ameos_pbsurvey_assessment/locallang_db.xml:flexform.message</label>
												<config>
												   <type>text</type>
												   <cols>30</cols>
												   <rows>5</rows>
												</config>
											</TCEforms>
										</contenu>
									</el>
								</pallier>
							</el>
						</palliers>
					</el>
				</ROOT>
		</sheet>
	</sheets>
</T3DataStructure>